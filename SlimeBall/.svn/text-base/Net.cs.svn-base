using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace SlimeBall
{
    /// <summary>
    /// Summary for Net.
    /// </summary>
    class Net : Sprite
    {
        /// <summary>
        /// Default constructor.
        /// </summary>
        public Net() : base() { }
        
        /// <summary>
        /// Constructor that accepts filename of image.
        /// </summary>
        /// <param name="fileName"></param>
        public Net(string fileName) : base(fileName) {}

        /// <summary>
        /// Constructor that accepts all arguments needed for net object.
        /// </summary>
        /// <param name="fileName">Filename of image.</param>        
        /// <param name="ptX">Initial position : X coordinate</param>
        /// <param name="ptY">Initial position : Y coordinate</param>
        /// <param name="mass">Mass of the slimer</param>        
        public Net(string fileName, int ptX=0, int ptY=0) : base(fileName, ptX, ptY) {}

        /// <summary>
        /// Draws the net.
        /// </summary>
        /// <param name="g"></param>
        public override void Draw(System.Drawing.Graphics g)
        {
            base.Draw(g);
        }
    }
}
